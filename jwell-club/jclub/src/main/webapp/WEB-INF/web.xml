<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>jclub</display-name>
	
	<filter>
		<filter-name>jwell</filter-name>
		<filter-class>com.fenwell.jwell.FilterDispatcher</filter-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>spring.xml</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>jwell</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
</web-app>
